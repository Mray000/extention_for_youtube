{
    "name": "answers",
    "description": "PrP",
    "version": "2.0",
    "manifest_version": 3,
    "content_scripts": [
      {
          "matches": [
            "*://*.youtube.com/*"
          ],
          "js": ["content.js"]
      }
      ],
    "action": {
        "default_popup": "popup.html"
      },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "webRequest"
    ],
    "host_permissions": [
      "*://*.youtube.com/*"
  ]
}